<div class="box_2">
	<h2 class="title"><%= t("label.item_new", :item => User.model_name.human) %></h2><br>
	<%= form_for @user do |f| %>
		<%= render :partial => "form", :locals => {:f => f, :user => @user} %>
		<br>
		<div align="center">
				<%= f.submit t("single.create").capitalize, :class => "button_1" %>
		</div>
	<% end %>
</div>
<%= back %>
